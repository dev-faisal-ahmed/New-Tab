{
  "manifest_version": 3,
  "name": "Search-UIX",
  "description": "A quick way to search.",
  "version": "0.0.1",
  "permissions": ["activeTab"],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "Web",
      "keyword": "keyword.__MSG_url_domain__",
      "search_url": "https://searchuix.onrender.com/search?q={searchTerms}",
      "favicon_url": "https://google.com/favicon.ico",
      "encoding": "UTF-8",
      "is_default": true
    }
  }
}
